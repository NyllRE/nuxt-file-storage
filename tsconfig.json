{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"target": "es2020",
		"module": "node16",
		"moduleResolution": "nodenext",
		"esModuleInterop": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": false,
		"resolveJsonModule": true,
		"isolatedModules": false
	},
	"include": ["test", "src", ".nuxt"]
}
